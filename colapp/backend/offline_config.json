{
  "ocr": {
    "engine": "auto",
    "tesseract_config": "--oem 3 --psm 6"
  },
  "llm": {
    "model": "llama2:7b",
    "host": "http://host.docker.internal:11434",
    "temperature": 0.1,
    "max_tokens": 2048
  },
  "custom_nlp": {
    "enabled": true,
    "confidence_threshold": 0.7
  },
  "file_processing": {
    "max_file_size": 10485760,
    "supported_formats": [
      ".jpg",
      ".jpeg",
      ".png",
      ".pdf"
    ],
    "temp_dir": "./temp"
  }
}